{
  "name": "axios-nice-log",
  "version": "0.0.3",
  "description": "Axios interceptor logger for requests query parameters",
  "author": "reslear <reslear@gmail.com>",
  "homepage": "https://github.com/reslear/whale/tree/main/packages/axios-nice-log",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "log",
    "axios",
    "logging",
    "chalk"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reslear/whale.git"
  },
  "scripts": {
    "dev": "tsnd --respawn examples/express/server.ts",
    "build": "tsup src/index.ts --format esm,cjs --dts --external axios,qs,chalk,chalk-pipe",
    "clean": "rm -rf dist",
    "deploy": "npm run clean && npm run build && npm version patch && npm publish --access public"
  },
  "bugs": {
    "url": "https://github.com/reslear/whale/issues"
  },
  "devDependencies": {
    "@types/chalk-pipe": "^2.0.0",
    "@types/express": "^4.17.9",
    "axios": "0.21.1",
    "express": "^4.17.1",
    "ts-node": "9.1.1",
    "ts-node-dev": "^1.1.1",
    "tsup": "3.11.0",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "chalk-pipe": "^4.0.0"
  }
}
