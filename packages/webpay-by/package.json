{
  "name": "webpay-by",
  "version": "0.0.1",
  "description": "Form fields api for Belarus payment system Webpay.by",
  "author": "reslear <reslear@gmail.com>",
  "homepage": "https://github.com/reslear/whale/tree/main/packages/webpay-by",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reslear/whale.git"
  },
  "scripts": {
    "parse": "ts-node scripts/generate-types/index.ts",
    "dev": "tsnd --respawn examples/express/server.ts",
    "build": "tsup src/index.ts --format esm,cjs --dts --external axios",
    "clean": "rm -rf dist",
    "deploy": "npm run clean && npm run build && npm version patch && npm publish --access public"
  },
  "bugs": {
    "url": "https://github.com/reslear/whale/issues"
  },
  "devDependencies": {
    "ts-node": "9.1.1",
    "ts-node-dev": "^1.1.1",
    "tsup": "3.11.0",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "node-html-parser": "^2.0.0"
  }
}
